{
  "dart.flutterSdkPath": ".fvm/flutter_sdk",
  "search.exclude": {
    "**/.git": true,
    "**/.dart_tool": true,
    "**/.fvm": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true
  },
  // Remove from file watching
  "files.watcherExclude": {
    "**/.fvm": true
  },
  "files.exclude": {
    "**/.git": false,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.dart_tool": true,
    "**/.github": true,
    "**/.vscode": true,
    "**/.fvm": true,
    "**/.flutter-plugins-dependencies": true,
    "**/.flutter-plugins": true,
    "**/.packages": true,
    "**/coverage": true
  },
  "explorerExclude.backup": {}
}